{
    "name": "lifecycle",
    "version": "1.0.0",
    "description": "Chaincode lifecycle",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "compile": "tsc --build tsconfig.json",
        "cucumber": "rimraf lib/cucumber && npm run compile && ./node_modules/.bin/cucumber-js ./cucumber/features/ -p default"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-ca-client": "unstable",
        "fabric-network": "unstable",
        "fabric-common": "unstable",
        "ignore-walk": "^3.0.0",
        "klaw": "^2.0.0",
        "tar-stream": "1.6.1"
    },
    "devDependencies": {
        "@types/chai": "^4.2.10",
        "@types/chai-as-promised": "^7.1.2",
        "@types/cucumber": "^6.0.1",
        "@types/fs-extra": "^8.1.0",
        "@types/mocha": "^7.0.2",
        "@types/sinon": "^7.5.2",
        "@types/sinon-chai": "^3.2.3",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "cucumber": "^6.0.5",
        "eslint": "^5.9.0",
        "fs-extra": "^8.1.0",
        "mocha": "^5.2.0",
        "nyc": "^14.1.1",
        "rimraf": "^3.0.2",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.3.0",
        "typescript": "^3.7.2"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    },
    "types": "./types/index"
}
